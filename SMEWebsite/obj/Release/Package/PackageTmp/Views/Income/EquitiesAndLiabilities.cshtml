    function InsertEquiLib() {
        var cnt = $('#ddlyear').val();

        var EquitiesLiabilities = new Array();

        for (var i = 1; i <= cnt; i++) {

            var Param_User = {
              
                CompId: $('#ddlcompany').val(),
                Year: $('#txtyear_' + i).val(),
                ShareCapital: $('#txtsharecapital_' + i).val(),
                ReserveSurplus: $('#txtreserveandsurplus_' + i).val(),
                TotalNetWorth: $('#txttotalnetworth_' + i).val(),
                LongBorrowings: $('#txtlongtermborrowings_' + i).val(),
                LongLiabilities: $('#txtotherlongtermliabilities_' + i).val(),
                totalliabilities: $('#txttotallongtermliabilities_' + i).val(),
                bankfinance: $('#txtshorttermbank_' + i).val(),
                tradepayables: $('#txttradepayables_' + i).val(),
                statutorydues: $('#txtstatutorydues_' + i).val(),
                othercurrentliabilities: $('#txtothercurrentliabilities_' + i).val(),
                totalcurrentliabilities: $('#txtotalcurrentliabilities_' + i).val(),
                totalequitiesliabilities: $('#txtotalequitiesliabilities_' + i).val(),
                netfiextangible: $('#txtnetfixedassetstangible_' + i).val(),
                netfixedintangible: $('#txtnetfixedassetsintangible_' + i).val(),
                capitalwork: $('#txtcapitalworkprogress_' + i).val(),
                othernoncurrentAssets: $('#txtothernoncurrentassets_' + i).val(),
                currentAssets: $('#txtothercurrentassets_' + i).val(),
                RawMaterials: $('#txtrawmaterial_' + i).val(),
                WIP: $('#txtwip_' + i).val(),
                storesConsumables: $('#txtstoreandconsumables_' + i).val(),
                FinishedGoods: $('#txtfinishedgoods_' + i).val(),
                TotalInventories: $('#txttotalinventories_' + i).val(),
                Lessdays: $('#txtlessthan90days_' + i).val(),
                Moredays: $('#txtmorethan90days_' + i).val(),
                TotalDebtors: $('#txttotaldebtors_' + i).val(),
                ShortTermloans: $('#txtshorttermloansandadvances_' + i).val(),
                OthercurrentAssets: $('#txtothercurrentassets_' + i).val(),
                TotalAssets: $('#txttotalassets_' + i).val(),
                check: $('#txtcheck_' + i).val()                
            }
            EquitiesLiabilities.push(Param_User);
        }

        $.ajax({
            type: "POST",/*method type*/
            contentType: "application/json; charset=utf-8",
            url: "/Income/InsertEquitiesAndLiabilities",/*Target function that will be return result*/
            data: JSON.stringify(EquitiesLiabilities),
            dataType: "json",
            success: function (data) {
                

                 data = JSON.parse(data);
                 var HTML = "";
                $("#tdCUGrid").empty();
                for (var i = 0; i < data.length; i++) {                    
                    HTML = HTML + '<tr>';
                    
                    HTML = HTML + '<td align="left" style="padding-center: 0px" class="hidden" width="0%">' + data[i].Id + '</td><td align="left" style="padding-left: 4px"  width="7%">' + parseInt(i + 1) + '</td>';
                    HTML = HTML + '<td align="left" style="padding-left: 4px"  width="15%">' + data[i].CompanyName + '</td><td align="left" style="padding-left: 4px"  width="12%">' + data[i].Years + '</td>';
                    
                    HTML = HTML + '<td align="left" style="padding-left: 4px"  width="20%">' + data[i].Total_Equities_Liabilities + '</td>';
                    HTML = HTML + '<td align="left" style="padding-left: 4px"  width="8%">' + data[i].Total_Assets + '</td><td align="left" style="padding-left: 4px"  width="8%">' + data[i].Checked + '</td>';
                    
                    HTML = HTML + '<td align="left" style="padding-left: 4px"  width="8%"><span id="promotorspan" onclick="Download(' + data[i].Id + ',' + data[i].Company + ')"><a class="delete" id="deleteac" href="javascript:void(0)">Download</a></span></td>';
                    HTML = HTML + '<td align="left" style="padding-left: 4px"  width="8%"><span id="promotorspan" onclick="deleteEquilib(' + data[i].Id + ',' + data[i].Company + ')"><a class="delete" id="deleteac" href="javascript:void(0)">Delete</a></span></td>';
                    HTML = HTML + '</tr>';                    
                }
                $("#tdCUGrid").html(HTML);
                $('#tdCUGrid tr').hover(function () {
                    $(this).addClass('highlight');
                }, function () {
                    $(this).removeClass('highlight');
                });
            },

            error: function (result) {
                alert(result);
            }
        });
    }

