    //Convert int to Company Type
    function Type(Type) {
        return Type = 1 ? 'OD' : Type = 2 ? 'CC' : Type = 3 ? 'LC' :
               Type = 4 ? 'BG' : Type = 5 ? 'BUYER CREDIT' : Type = 6 ? 'EPC' :
               Type = 7 ? 'FPC' : 'N/A';
    }

    //Convert int to Company Type
    function Collateral(Type) {
        return Type = 1 ? 'Residential' : Type = 2 ? 'Commercial' : Type = 3 ? 'Industrial' : 'Other';
    }

    //Convert int to Company Type
    function FacilitiesTypes(Type) {
        return Type = 1 ? 'Net Sale' : Type = 2 ? 'Net Profit' : Type = 3 ? 'Promoter Influence' : 'N/A';
    }

    //Convert int to Company Type
    function CompanyOwner(CompanyOwner) {
        return CompanyOwner = 1 ? 'Proprietor' : CompanyOwner = 2 ? 'Partner' : 'Promoter Directors';
    }

    //Convert int to Company Type
    function Gender(Gender) {
        return Gender = '1' ? 'Male' : 'Female';//Type = 'm' ? 'Male' : 'N/A';
    }

