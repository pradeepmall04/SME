    function GetDatenew(value) {

        var newspl = "";
        if (value != null) {
            var split = value.split('T');
            var results = split[0];

            var splitD = results.split("-");

            newspl = splitD[2] + '-' + splitD[1] + '-' + splitD[0];
        }

        return newspl;
    }

