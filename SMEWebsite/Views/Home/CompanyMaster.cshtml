    function downloadURL(test) {

        var name = $("#example").children().children()[test - 1].children[2].innerHTML
        var docs = $("#example").children().children()[test - 1].children[4].innerHTML


        var url = document.getElementById("hdnfile" + test).value;
        var obj = {};
        obj.path = url;
        var ext = obj.path.split('.');
        $.ajax({
            type: "POST",/*method type*/
            contentType: "application/json; charset=utf-8",
            url: "/Home/Download",/*Target function that will be return result*/
            data: JSON.stringify(obj),//'{"compid":"' + compid + '","userid":"' + userid + '","filepath":"' + filepath + '","check":"Y"}',/*parameter pass data is parameter name param is value */
            dataType: "json",
            success: function (data) {
                var pdf = 'data:application/octet-stream;base64,' + data;
                var dlnk = document.createElement('a');
                dlnk.download = name + "_" + docs + "."+ext[1];
                dlnk.href = pdf;

                dlnk.click();

            },

            error: function (result) {
                alert(result);
            }
        });
    }


